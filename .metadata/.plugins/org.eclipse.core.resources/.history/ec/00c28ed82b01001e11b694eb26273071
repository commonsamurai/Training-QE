export async function highestFee(associateId) {	 	  	  		    	   	 	   	 	
  //  console.log("view all course",localStorage.getItem("token"))
  const options = {
    headers,    
  };
  const response = await fetch('http://localhost:9093/admission/highestFee/'+associateId, options);
  return await response.json();
  
}

export async function viewFeedbackByCourseId(courseId) {
  //  console.log("view all course",localStorage.getItem("token"))
  const options = {
    headers,   
  };
  const response = await fetch('http://localhost:9093/admission/viewFeedbackByCourseId/'+courseId, options);
  return await response.json();
  
}
  
export async function makePayment(registartionId) {
   console.log("feed",localStorage.getItem("token"))
  const options = {
    headers,
  };
  const response = await fetch('http://localhost:9093/admission/makePayment/'+registartionId, options);
  return await response.json();
  
}

	 	  	  		    	   	 	   	 	
