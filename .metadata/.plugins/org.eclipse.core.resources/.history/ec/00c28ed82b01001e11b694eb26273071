
let headers= {
    'Authorization': "Bearer "+localStorage.getItem("token"),
    'Content-Type': 'application/json',
    "Access-Control-Allow-Methods": "POST, PUT, GET,OPTIONS,DELETE" ,      
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Headers": "Origin,Access-Control-Allow-Origin, X-Requested-With, Content-Type, Accept,Authorization"          

  }

export  async function register(associateId,courseId) {
    const response = await fetch('http://localhost:9093/register/'+associateId+"/"+courseId, {
    method: 'POST',
    headers,    
    body: JSON.stringify(associateId)
  })

  return await response.json();  
}

export async function calculateFees(courseId) {
  const response = await fetch('http://localhost:9093/admission/calculateFees/'+courseId, {
  method: 'PUT',
  headers,
  body: JSON.stringify(courseId)
})

return await response.json();  
}

export  async function addfeedback(regNo,feedback,feedbackRating) {
    const response = await fetch('http://localhost:9093/admission/feedback/'+regNo+"/"+feedback+"/"+feedbackRating, {
    method: 'POST',
    headers,
    body: JSON.stringify(regNo)
  })

  return await response.json();  
}	 	  	  		    	   	 	   	 	

